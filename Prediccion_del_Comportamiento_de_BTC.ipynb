        "# Importación de librerías necesarias\n",
        "import pandas as pd\n",
        "import numpy as np\n",
        "import requests\n",
        "from sklearn.model_selection import train_test_split, cross_val_score\n",
        "from sklearn.preprocessing import StandardScaler\n",
        "from sklearn.metrics import classification_report, accuracy_score, roc_auc_score, confusion_matrix, ConfusionMatrixDisplay\n",
        "from imblearn.over_sampling import SMOTE\n",
        "import xgboost as xgb\n",
        "import matplotlib.pyplot as plt\n",
        "import optuna\n",
        "import joblib\n",
        "\n",
        "# === Funciones para indicadores técnicos ===\n",
        "\n",
        "# Calcula la volatilidad histórica basado en la desviación estándar de los cambios porcentuales\n",
        "def calculate_historical_volatility(data, window=14):\n",
        "    return data.pct_change().rolling(window).std()\n",
        "\n",
        "# Calcula el Índice de Fuerza Relativa (RSI) para determinar condiciones de sobrecompra o sobreventa\n",
        "def calculate_rsi(data, window=14):\n",
        "    delta = data.diff()  # Calcula la diferencia entre valores consecutivos\n",
        "    gain = (delta.where(delta > 0, 0)).rolling(window=window).mean()  # Promedio de ganancias\n",
        "    loss = (-delta.where(delta < 0, 0)).rolling(window=window).mean()  # Promedio de pérdidas\n",
        "    rs = gain / loss  # Relación de ganancia y pérdida\n",
        "    rsi = 100 - (100 / (1 + rs))  # Cálculo del RSI\n",
        "    return rsi\n",
        "\n",
        "# Calcula el MACD (Moving Average Convergence Divergence), su señal y la diferencia entre ambos\n",
        "def calculate_macd(data, fast_period=12, slow_period=26, signal_period=9):\n",
        "    ema_fast = data.ewm(span=fast_period, adjust=False).mean()  # EMA rápida\n",
        "    ema_slow = data.ewm(span=slow_period, adjust=False).mean()    # EMA lenta\n",
        "    macd = ema_fast - ema_slow  # Línea MACD\n",
        "    signal = macd.ewm(span=signal_period, adjust=False).mean()  # Línea de señal\n",
        "    macd_diff = macd - signal  # Diferencia entre MACD y señal\n",
        "    return macd, signal, macd_diff\n",
        "\n",
        "# Calcula el ATR (Average True Range) que mide la volatilidad del mercado\n",
        "def calculate_atr(high, low, close, window=14):\n",
        "    high_low = high - low\n",
        "    high_close = (high - close.shift()).abs()\n",
        "    low_close = (low - close.shift()).abs()\n",
        "    # Calcula el True Range (TR) usando el máximo entre las diferencias\n",
        "    tr = high_low.combine(high_close, max).combine(low_close, max)\n",
        "    atr = tr.rolling(window=window).mean()  # Promedio del TR en la ventana\n",
        "    return atr\n",
        "\n",
        "# Calcula el momentum o impulso, midiendo la diferencia con un valor anterior\n",
        "def calculate_momentum(data, window=10):\n",
        "    return data.diff(window)\n",
        "\n",
        "# Calcula las bandas de Bollinger, que indican niveles de sobrecompra y sobreventa\n",
        "def calculate_bollinger_bands(data, window=20, num_std=2):\n",
        "    # Si existen valores NaN, se rellenan con 0 para evitar errores\n",
        "    if data.isnull().any():\n",
        "        print(\"Datos de entrada contienen NaN, rellenando con 0.\")\n",
        "        data = data.fillna(0)\n",
        "    sma = data.rolling(window=window).mean()  # Media móvil simple\n",
        "    std = data.rolling(window=window).std()   # Desviación estándar\n",
        "    upper_band = sma + (std * num_std)  # Banda superior\n",
        "    lower_band = sma - (std * num_std)  # Banda inferior\n",
        "    # Rellenamos posibles NaN en las bandas\n",
        "    upper_band = upper_band.fillna(0)\n",
        "    lower_band = lower_band.fillna(0)\n",
        "    return upper_band, lower_band\n",
        "\n",
        "# Calcula la media exponencial (EMA)\n",
        "def calculate_ema(data, span=20):\n",
        "    return data.ewm(span=span, adjust=False).mean()\n",
        "\n",
        "# Calcula el Force Index que combina cambios de precio con volumen\n",
        "def calculate_force_index(close, volume):\n",
        "    return (close.diff() * volume).rolling(window=2).mean()\n",
        "\n",
        "# Calcula el indicador Williams %R para medir condiciones de sobrecompra o sobreventa\n",
        "def calculate_williams_r(high, low, close, window=14):\n",
        "    highest_high = high.rolling(window=window).max()  # Máximo en la ventana\n",
        "    lowest_low = low.rolling(window=window).min()       # Mínimo en la ventana\n",
        "    denominator = highest_high - lowest_low             # Rango en la ventana\n",
        "    denominator = denominator.replace(0, np.nan)        # Evita divisiones por cero\n",
        "    williams_r = -100 * ((highest_high - close) / denominator)  # Cálculo de Williams %R\n",
        "    williams_r = williams_r.fillna(0)  # Rellena NaN\n",
        "    return williams_r\n",
        "\n",
        "# Calcula la volatilidad relativa en función de la desviación estándar móvil\n",
        "def calculate_relative_volatility(data, window=14):\n",
        "    rolling_std = data.rolling(window=window).std()\n",
        "    rolling_std = rolling_std.fillna(0)\n",
        "    mean_volatility = rolling_std.mean()\n",
        "    mean_volatility = max(mean_volatility, 1e-10)  # Evita divisiones por cero\n",
        "    relative_volatility = rolling_std / mean_volatility\n",
        "    return relative_volatility\n",
        "\n",
        "# Calcula la tasa de cambio (Rate of Change) en un lapso de tiempo\n",
        "def calculate_rate_of_change(data, window=14):\n",
        "    shifted_data = data.shift(window)\n",
        "    with np.errstate(divide='ignore', invalid='ignore'):\n",
        "        rate_of_change = (data / shifted_data) - 1\n",
        "        # Reemplaza valores infinitos o NaN por 0\n",
        "        rate_of_change = rate_of_change.replace([np.inf, -np.inf], np.nan).fillna(0)\n",
        "    return rate_of_change\n",
        "\n",
        "# Calcula la línea de acumulación/distribución que mide el flujo de dinero\n",
        "def calculate_accumulation_distribution(close, high, low, volume):\n",
        "    money_flow_multiplier = ((close - low) - (high - close)) / (high - low)\n",
        "    money_flow_volume = money_flow_multiplier * volume\n",
        "    ad_line = money_flow_volume.cumsum()  # Suma acumulada del flujo de dinero\n",
        "    return ad_line\n",
        "\n",
        "# Calcula el Money Flow Index (MFI), un indicador que combina precio y volumen\n",
        "def calculate_mfi(high, low, close, volume, window=14):\n",
        "    typical_price = (high + low + close) / 3\n",
        "    money_flow = typical_price * volume\n",
        "    # Flujos de dinero positivo y negativo\n",
        "    positive_flow = money_flow.where(typical_price > typical_price.shift(), 0).rolling(window).sum()\n",
        "    negative_flow = money_flow.where(typical_price < typical_price.shift(), 0).rolling(window).sum()\n",
        "    negative_flow = negative_flow.replace({0: np.nan}).fillna(1e-10)\n",
        "    mfi = 100 - (100 / (1 + positive_flow / negative_flow))\n",
        "    return mfi.fillna(0)\n",
        "\n",
        "# Calcula el ADX (Average Directional Index) que mide la fuerza de la tendencia\n",
        "def calculate_adx(high, low, close, window=14):\n",
        "    atr = calculate_atr(high, low, close, window)  # ATR como base para el cálculo\n",
        "    up_move = high.diff()    # Movimiento hacia arriba\n",
        "    down_move = low.diff()   # Movimiento hacia abajo\n",
        "    # Cálculo del movimiento direccional positivo y negativo\n",
        "    plus_dm = pd.Series(np.where((up_move > down_move) & (up_move > 0), up_move, 0), index=high.index)\n",
        "    minus_dm = pd.Series(np.where((down_move > up_move) & (down_move > 0), down_move, 0), index=low.index)\n",
        "    # Índices direccionales\n",
        "    plus_di = 100 * (plus_dm.rolling(window).sum() / atr.replace({0: np.nan}))\n",
        "    minus_di = 100 * (minus_dm.rolling(window).sum() / atr.replace({0: np.nan}))\n",
        "    dx = 100 * abs(plus_di - minus_di) / (plus_di + minus_di).replace({0: np.nan})\n",
        "    adx = dx.rolling(window).mean()  # Promedio del DX para obtener el ADX\n",
        "    return adx.fillna(0)\n",
        "\n",
        "# === 1. Función para Obtener Datos ===\n",
        "def fetch_crypto_data(endpoint='/time-series/v2', topic=None, coin_id=None):\n",
        "    # URL base de la API de LunarCrush\n",
        "    base_url = \"https://lunarcrush.com/api4/public/\"\n",
        "    # Encabezados con token de autorización\n",
        "    headers = {'Authorization': 'Bearer iraqfuqfl9ohcdbwzw1gi6p9em5a9pivw6xwov5'}\n",
        "\n",
        "    # Se construye la URL según si se utiliza 'topic' o 'coin_id'\n",
        "    if topic:\n",
        "        url = f\"{base_url}topic/{topic}{endpoint}\"\n",
        "    elif coin_id:\n",
        "        url = f\"{base_url}coins/{coin_id}{endpoint}\"\n",
        "    else:\n",
        "        raise ValueError(\"Debes proporcionar un 'topic' o 'coin_id'\")\n",
        "\n",
        "    # Se realiza la solicitud HTTP\n",
        "    response = requests.get(url, headers=headers)\n",
        "    if response.status_code == 200:\n",
        "        data = response.json()\n",
        "        # Se convierte la parte 'data' de la respuesta a un DataFrame\n",
        "        if 'data' in data:\n",
        "            df = pd.DataFrame(data['data'])\n",
        "            # Convierte la columna 'time' a tipo datetime\n",
        "            if 'time' in df.columns:\n",
        "                df['time'] = pd.to_datetime(df['time'], unit='s')\n",
        "            return df\n",
        "    else:\n",
        "        raise Exception(f\"Error al obtener datos: {response.status_code}\")\n",
        "\n",
        "# === 2. Función de Procesamiento ===\n",
        "def process_data(df, freq='D'):\n",
        "    # Se eliminan columnas no necesarias\n",
        "    drop_cols = ['volatility', 'spam', 'circulating_supply', 'market_dominance', 'volume_24h']\n",
        "    df = df.drop(columns=drop_cols, errors='ignore')\n",
        "\n",
        "    # Conversión y ajuste de la columna 'time'\n",
        "    df['time'] = pd.to_datetime(df['time'])\n",
        "    df.set_index('time', inplace=True)\n",
        "\n",
        "    # Re-muestreo de los datos según la frecuencia indicada (por defecto diaria)\n",
        "    df = df.resample(freq).agg({\n",
        "        'open': 'first',\n",
        "        'high': 'max',\n",
        "        'low': 'min',\n",
        "        'close': 'last',\n",
        "        'market_cap': 'mean'\n",
        "    }).dropna()\n",
        "\n",
        "    # Cálculo del cambio porcentual en el precio y creación de la variable 'target'\n",
        "    df['price_change'] = df['close'].pct_change().shift(-1)\n",
        "    threshold_buy = 0.02   # Umbral para señal de compra\n",
        "    threshold_sell = -0.02 # Umbral para señal de venta\n",
        "    df['target'] = np.select([\n",
        "        df['price_change'] > threshold_buy,\n",
        "        df['price_change'] < threshold_sell\n",
        "    ], [1, 2], default=0)  # 1: Buy, 2: Sell, 0: Hold\n",
        "\n",
        "    # Cálculo de indicadores técnicos y nuevas características\n",
        "    df['atr'] = calculate_atr(df['high'], df['low'], df['close']).fillna(0)\n",
        "    df['historical_volatility'] = calculate_historical_volatility(df['close']).fillna(0)\n",
        "    df['historical_volatility'] = df['historical_volatility'].replace({0: np.nan}).fillna(1e-10)\n",
        "    df['atr_volatility'] = df['atr'] / df['historical_volatility']\n",
        "    df['atr_volatility'] = df['atr_volatility'].fillna(0)\n",
        "    df['bollinger_upper'], df['bollinger_lower'] = calculate_bollinger_bands(df['close'])\n",
        "    df['bollinger_upper'] = df['bollinger_upper'].fillna(0)\n",
        "    df['bollinger_lower'] = df['bollinger_lower'].fillna(0)\n",
        "    df['ema_50'] = calculate_ema(df['close'], span=50)\n",
        "    df['ema_200'] = calculate_ema(df['close'], span=200)\n",
        "    df['williams_r'] = calculate_williams_r(df['high'], df['low'], df['close'])\n",
        "    df['relative_volatility'] = calculate_relative_volatility(df['close'])\n",
        "    df['rate_of_change'] = calculate_rate_of_change(df['close'])\n",
        "    df['accumulation_distribution'] = calculate_accumulation_distribution(df['close'], df['high'], df['low'], df['market_cap'])\n",
        "    df['mfi'] = calculate_mfi(df['high'], df['low'], df['close'], df['market_cap'])\n",
        "    df['adx'] = calculate_adx(df['high'], df['low'], df['close'])\n",
        "\n",
        "    return df\n",
        "\n",
        "# === 3. Función para Entrenar el Modelo ===\n",
        "def train_model(X, y):\n",
        "    # Aplicación de SMOTE para balancear las clases en el conjunto de datos\n",
        "    smote = SMOTE(random_state=42)\n",
        "    X_resampled, y_resampled = smote.fit_resample(X, y)\n",
        "\n",
        "    # División de los datos en conjuntos de entrenamiento y prueba\n",
        "    X_train, X_test, y_train, y_test = train_test_split(X_resampled, y_resampled, test_size=0.2, random_state=42)\n",
        "\n",
        "    # Escalado de los datos para mejorar el rendimiento del modelo\n",
        "    scaler = StandardScaler()\n",
        "    X_train_scaled = scaler.fit_transform(X_train)\n",
        "    X_test_scaled = scaler.transform(X_test)\n",
        "\n",
        "    # Parámetros predefinidos para el modelo XGBoost (se pueden optimizar con optuna)\n",
        "    best_params = {\n",
        "        'n_estimators': 907,\n",
        "        'max_depth': 14,\n",
        "        'learning_rate': 0.018944056221819314,\n",
        "        'subsample': 0.8335735764811254,\n",
        "        'colsample_bytree': 0.5141924607282815,\n",
        "        'min_child_weight': 1\n",
        "    }\n",
        "    best_model = xgb.XGBClassifier(**best_params, random_state=42)\n",
        "    best_model.fit(X_train_scaled, y_train)\n",
        "\n",
        "    # Predicción en el conjunto de prueba y cálculo de métricas\n",
        "    y_pred = best_model.predict(X_test_scaled)\n",
        "    print(f\"Precisión en Test: {accuracy_score(y_test, y_pred)}\")\n",
        "\n",
        "    # Cálculo del ROC AUC para clasificación multiclase\n",
        "    y_pred_proba = best_model.predict_proba(X_test_scaled)\n",
        "    roc_auc = roc_auc_score(y_test, y_pred_proba, multi_class='ovr')\n",
        "    print(f\"ROC AUC Score: {roc_auc}\")\n",
        "\n",
        "    # Reporte de clasificación\n",
        "    print(classification_report(y_test, y_pred))\n",
        "\n",
        "    # Cálculo y visualización de la matriz de confusión\n",
        "    cm = confusion_matrix(y_test, y_pred)\n",
        "    disp = ConfusionMatrixDisplay(confusion_matrix=cm, display_labels=[0, 1, 2])\n",
        "    disp.plot(cmap='Blues')\n",
        "    plt.title('Matriz de Confusión')\n",
        "    plt.show()\n",
        "\n",
        "    # Guardado del modelo entrenado en un archivo\n",
        "    joblib.dump(best_model, 'xgb_model.pkl')\n",
        "    print(\"Modelo guardado como 'xgb_model.pkl'\")\n",
        "\n",
        "    return best_model, scaler\n",
        "\n",
        "# === 4. Función para Visualizar la Importancia de las Características ===\n",
        "def plot_feature_importance(model, features):\n",
        "    importance = model.feature_importances_  # Obtiene la importancia relativa de cada característica\n",
        "    sorted_idx = np.argsort(importance)        # Ordena los índices según la importancia\n",
        "    plt.figure(figsize=(10, 6))\n",
        "    plt.barh(range(len(importance)), importance[sorted_idx], align='center')\n",
        "    plt.yticks(range(len(importance)), [features[i] for i in sorted_idx])\n",
        "    plt.xlabel('Importancia Relativa')\n",
        "    plt.title('Importancia de Características')\n",
        "    plt.show()\n",
        "\n",
        "# === 5. Pipeline Principal ===\n",
        "try:\n",
        "    # Obtención de datos desde la API de LunarCrush\n",
        "    df = fetch_crypto_data(endpoint='/time-series/v2?bucket=hour&interval=5y&start=1577847600&end=1735614000', coin_id='1')\n",
        "    # Procesamiento de los datos obtenidos\n",
        "    df = process_data(df)\n",
        "\n",
        "    # Selección de las características y definición de la variable target\n",
        "    features = ['open', 'high', 'low', 'market_cap', 'atr_volatility', 'bollinger_upper', 'bollinger_lower', 'williams_r',\n",
        "                'relative_volatility', 'rate_of_change', 'accumulation_distribution', 'mfi', 'adx']\n",
        "    X = df[features]\n",
        "    y = df['target']\n",
        "\n",
        "    # Entrenamiento del modelo y escalado de los datos\n",
        "    model, scaler = train_model(X, y)\n",
        "\n",
        "    # Visualización de la importancia de las características\n",
        "    plot_feature_importance(model, features)\n",
        "\n",
        "except Exception as e:\n",
        "    # En caso de error, se imprime el mensaje correspondiente\n",
